.background {
}

.option-sector {
    background-image: resource('Sprites/sector');
    -unity-background-image-tint-color: rgb(255, 255, 255);
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    transition-property: opacity, -unity-background-image-tint-color;
    transition-duration: 0.1s, 0s;
    opacity: 0;
}
